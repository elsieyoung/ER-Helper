<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.triage"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SearchActivity"
            android:label="@string/title_activity_search" >
        </activity>
        <activity
            android:name=".DisplayActivity"
            android:label="@string/title_activity_display" >
        </activity>

     
     
        <activity
            android:name=".ViewVisitActivity"
            android:label="@string/title_activity_view_visit" >
        </activity>
        <activity
            android:name=".AddVitalSignsActivity"
            android:label="@string/title_activity_add_vital_signs" >
        </activity>
        <activity
            android:name=".VitalRecordActivity"
            android:label="@string/title_activity_vital_record" >
        </activity>
        <activity
            android:name=".DisplayPhysicianActivity"
            android:label="@string/title_activity_display_physician" >
        </activity>
        <activity
            android:name=".AddPrescriptionActivity"
            android:label="@string/title_activity_add_prescription" >
        </activity>
        <activity
            android:name=".PhysicianSearchActivity"
            android:label="@string/title_activity_physician_search" >
        </activity>
        <activity
            android:name=".AddPatientActivity"
            android:label="@string/title_activity_add_patient" >
        </activity>
       
    </application>

</manifest>
